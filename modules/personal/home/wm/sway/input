input type:keyboard {
	xkb_layout gb
	xkb_numlock enable
}

input type:pointer {
	accel_profile flat
	pointer_accel 0
}

input type:touchpad {
	tap enabled
	natural_scroll enabled
	dwt enabled
	pointer_accel 0.2
	click_method clickfinger
	scroll_factor 0.5
}

seat seat0 hide_cursor when-typing enable
