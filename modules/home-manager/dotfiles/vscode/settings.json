{
  "cSpell.language": "en-GB",
  "cSpell.userWords": [
    "automatable",
    "centricity",
    "composability",
    "conneted",
    "datacentre",
    "datacentres",
    "deallocate",
    "exokernels",
    "handoff",
    "implmentation",
    "inconvient",
    "limitted",
    "linearizable",
    "malloc",
    "multikernels",
    "overreliance",
    "paravirtualization",
    "retraversal",
    "siloed",
    "unikernels",
    "userspace",
    "virtuality",
    "Weiser",
    "Zigbee"
  ],
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.fontFamily": "monospace, 'NotoSansMono Nerd Font'",
  "editor.fontSize": 16,
  "editor.minimap.enabled": false,
  "editor.suggestFontSize": 16,
  "latex-workshop.latex.recipes": [
		{
			"name": "latexmk",
			"tools": [
				"latexmk"
			]
		},
    {
			"name": "latexmk -shell-escape",
			"tools": [
				"latexmk -shell-escape"
			]
		},
		{
			"name": "latexmk (latexmkrc)",
			"tools": [
				"latexmk_rconly"
			]
		},
		{
			"name": "latexmk (lualatex)",
			"tools": [
				"lualatexmk"
			]
		},
		{
			"name": "latexmk (xelatex)",
			"tools": [
				"xelatexmk"
			]
		},
	],
  "latex-workshop.latex.tools": [
    {
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-pdf",
        "-outdir=%OUTDIR%",
        "%DOC%"
      ],
      "command": "latexmk",
      "env": {},
      "name": "latexmk"
    },
    {
      "args": [
        "-shell-escape",
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-pdf",
        "-outdir=%OUTDIR%",
        "%DOC%"
      ],
      "command": "latexmk",
      "env": {},
      "name": "latexmk -shell-escape"
    },
    {
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-lualatex",
        "-outdir=%OUTDIR%",
        "%DOC%"
      ],
      "command": "latexmk",
      "env": {},
      "name": "lualatexmk"
    },
    {
      "args": [
        "%DOC%"
      ],
      "command": "latexmk",
      "env": {},
      "name": "latexmk_rconly"
    },
    {
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "%DOC%"
      ],
      "command": "pdflatex",
      "env": {},
      "name": "pdflatex"
    },
    {
      "args": [
      "-shell-escape",
      "-synctex=1",
      "-interaction=nonstopmode",
      "-file-line-error",
      "%DOC%"
      ],
      "command": "pdflatex",
      "env": {},
      "name": "pdflatex -shell-escape"
    },
    {
      "args": [
        "%DOCFILE%"
      ],
      "command": "bibtex",
      "env": {},
      "name": "bibtex"
    },
  ],
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.latex.autoBuild.run": "onSave",
  "editor.wordWrap": "on",
  "latex-workshop.latex.recipe.default": "lastUsed",
  "grammarly.selectors": [
      {
          "language": "latex",
          "scheme": "file"
      }
  ],
  "grammarly.config.suggestions.StylisticFragments": true,
  "grammarly.config.suggestions.UnnecessaryEllipses": true,
  "grammarly.config.suggestions.PrepositionAtTheEndOfSentence": true,
  "grammarly.config.suggestions.PassiveVoice": true,
  "grammarly.config.suggestions.InformalPronounsAcademic": true,
  "grammarly.config.documentDomain": "academic",
  "grammarly.config.documentDialect": "british",
  "macros.list": {
    "nextError": [
      "editor.action.marker.next",
      "closeMarkersNavigation",
    ],
    "prevError": [
      "editor.action.marker.prev",
      "closeMarkersNavigation"
    ],
    "nextNextError": [
      "editor.action.marker.next",
      "editor.action.marker.next",
      "closeMarkersNavigation",
    ],
    "prevPrevError": [
      "editor.action.marker.prev",
      "editor.action.marker.prev",
      "closeMarkersNavigation"
    ],
  },
  "vscode-neovim.neovimExecutablePaths.linux": "nvim",
  "vscode-neovim.mouseSelectionStartVisualMode": true
}

